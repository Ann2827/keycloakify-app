{
    "homepage": "http://localhost:5000/",
    "name": "keycloakify-app",
    "version": "2.0.0",
    "description": "Authorization Application",
    "keywords": [
        "Keycloak",
        "SSO",
        "React",
        "Keycloakify"
    ],
    "author": "Ann Bystrova <ann.bystrova96@mail.ru>",
    "repository": {
        "type": "git",
        "url": "git://github.com/Ann2827/keycloakify-app.git"
    },
    "license": "MIT",
    "scripts": {
        "start:demo": "DISABLE_ESLINT_PLUGIN='true' ESLINT_NO_DEV_ERRORS='true' REACT_APP_MODE=demo react-scripts start",
        "build:theme1": "REACT_APP_MODE=build-theme1 react-scripts build && build-keycloak-theme",
        "build:theme2": "REACT_APP_MODE=build-theme2 react-scripts build && build-keycloak-theme",
        "build:demo": "REACT_APP_MODE=demo BUILD_PATH='./build_demo' react-scripts build",
        "rename:theme1": "mv build_keycloak/src/main/resources/theme/keycloakify-app build_keycloak/src/main/resources/theme/theme1-theme && mv build_keycloak build_theme1",
        "rename:theme2": "mv build_keycloak/src/main/resources/theme/keycloakify-app build_keycloak/src/main/resources/theme/theme2-theme && mv build_keycloak build_theme2",
        "build:themes": "npm run build:theme1 && npm run rename:theme1 && npm run build:theme2 && npm run rename:theme2",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint './src/**/*.{ts,tsx}'",
        "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix --quiet",
        "debug": "node scripts/debug-ftl.js --path=build_keycloak/src/main/resources/theme/keycloakify-app/login --file=login.ftl"
    },
    "engines": {
        "node": "^14.16.0",
        "npm": "^6.14.15"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "classnames": "^2.3.1",
        "evt": "^1.10.1",
        "keycloakify": "^5.6.3",
        "normalize.css": "^8.0.1",
        "powerhooks": "^0.10.0",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-router-dom": "^6.3.0",
        "sass": "^1.53.0",
        "tss-react": "^3.7.0"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/node": "^12.0.0",
        "@types/react": "18.0.9",
        "@types/react-dom": "18.0.4",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.2.0",
        "eslint": "^8.19.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-unicorn": "^37.0.1",
        "node-sass": "^7.0.1",
        "prettier": "^2.4.1",
        "react-scripts": "5.0.1",
        "typescript": "^4.1.2",
        "typescript-plugin-css-modules": "^3.4.0"
    }
}
